{
    "expo": {
        "jsEngine": "hermes",
        "name": "Jazida",
        "slug": "jazida-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "jsEngine": "hermes",
            "bundleIdentifier": "com.uncmplx.jazidaapp",
            "infoPlist": {
                "ITSAppUsesNonExemptEncryption": false
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.uncmplx.jazidaapp"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#F4C03E"
                }
            ],
            [
                "onesignal-expo-plugin",
                {
                    "mode": "development"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "28b4db64-0864-4249-9da3-660592c3920f"
            },
            "oneSignalAppId": "7da8c6f0-f5f2-447f-85f0-5171a9004bec"
        }
    }
}
